    "@testing-library/vue": "^8.1.0",
    "@types/d3": "^7.4.3",
    "@unocss/nuxt": "^0.63.4",
    "@vue/compiler-sfc": "^3.5.11",
    "cypress": "^13.15.0",
    "eslint": "^9.12.0",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "sass": "^1.79.4",
    "stylelint": "^16.9.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.6.2",
