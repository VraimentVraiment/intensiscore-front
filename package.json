{
  "name": "intensiscore-front",
  "description": "The front-end of the Intensi'Score project (intensiscore.m2intenses.com)",
  "version": "0.1.0",
  "author": "ShallowRed (Lucas Poulain)",
  "license": "GPL-3.0-only",
  "keywords": [
    "nuxt",
    "Intensi'Score"
  ],
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "start:back": "npm run start --prefix ${BACKEND_PATH:-'~/intensiscore-back'}",
    "test": "vitest",
    "test:ct": "cypress open --e2e",
    "test:ct-ci": "cypress run --e2e",
    "typecheck": "nuxi typecheck",
    "lint": "eslint .",
    "format": "eslint . --fix"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@nuxt/icon": "^1.5.5",
    "@nuxt/image": "^1.8.1",
    "@nuxtjs/seo": "2.0.0-rc.23",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.1",
    "@pinia/nuxt": "^0.5.5",
    "@vercel/analytics": "^1.3.1",
    "@vueuse/core": "^11.1.0",
    "d3": "^7.9.0",
    "html2canvas": "^1.4.1",
    "nuxt": "^3.13.2",
    "nuxt-directus": "^5.6.1",
    "nuxt-link-checker": "^3.1.2",
    "nuxt-security": "^2.0.0",
    "nuxt-seo-experiments": "^4.0.1",
    "pinia": "^2.2.4",
    "showdown": "^2.1.0",
